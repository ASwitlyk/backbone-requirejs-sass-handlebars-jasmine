{
  "name": "Backbone-Require-Handlebars-Jasmine",
  "version": "1.0.0",
  "description": "Backbone, Require, Handlebars and Jasmine testing",
  "main": "index.js",
  "scripts": {
    "sass": "sass src/sass/style.scss src/css/style.css",
    "sass-dev": "sass --watch src/sass/style.scss:src/css/style.css",
    "handlebars-precompile": "handlebars src/templates/** -f src/compiledTemplates/template.js -a -e hbs",
    "handlebars-precompile:watch": "watch \"npm run handlebars-precompile\" src/templates",
    "dev": "npm run sass-dev & npm run handlebars-precompile:watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "backbone": "^1.1.2",
    "handlebars": "^2.0.0",
    "jquery": "^2.1.3",
    "requirejs": "^2.1.15",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "jasmine": "^2.1.1"
  }
}
